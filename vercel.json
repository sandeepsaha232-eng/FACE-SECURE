{
    "version": 2,
    "builds": [
        {
            "src": "frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "https://face-secure-backend-production.up.railway.app/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}